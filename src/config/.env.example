# MongoDB Atlas connection string
MONGODB_URI=mongodb+srv://<username>:<password>@cluster0.mongodb.net/elevancenet?retryWrites=true&w=majority

# JWT secret key (must be strong and unique)
JWT_SECRET=your_jwt_secret_key_here

# JWT token expiry (in seconds)
JWT_EXPIRY=1800

# Log level (info, warn, error, debug)
LOG_LEVEL=info

# Email service config (for verification, if needed)
EMAIL_SERVICE_API_KEY=your_email_service_api_key_here
EMAIL_FROM=noreply@elevancenet.com

# Other environment variables as needed
